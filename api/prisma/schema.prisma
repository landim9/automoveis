generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Alocacao {
  Id Int @id @default(autoincrement())
  Area Int
  Automovel Int
  IdConcessionaria Int
  Quantidade Int
  concessionaria Concessionaria @relation(fields: [IdConcessionaria], references: [Id])
  automoveis Automoveis @relation(fields: [Automovel], references: [Id])
}

model Clientes {
  Id Int @id @default(autoincrement())
  Nome String @db.VarChar(255)
}

model  Automoveis {
  Id Int @id @default(autoincrement())
  Modelo String @db.VarChar(255)
  Preco Int
  Alocacao Alocacao[]
}

model Concessionaria {
  Id Int @id @default(autoincrement())
  Concessionaria String @db.VarChar(255)
  Alocacao Alocacao[]
}